AAFA,SAAA;AAIA,OAjBA;AAkBA;AApBA;AA3BA,CAAA","sourcesContent":["<template>\n  <div class=\"menu_switch_image\" :style=\"setStyle(prev, next)\">\n    <div @click.stop=\"switchImage('prev')\" :style=\"!prev && 'display:none'\">\n      <div class=\"icon\" v-html=\"icons.prev\"></div>\n    </div>\n    <div :style=\"!next && 'display:none'\" @click.stop=\"switchImage('next')\">\n      <div class=\"icon\" v-html=\"icons.next\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport icons from \"../icons.js\";\nexport default {\n  props: {\n    prev: {\n      type: Boolean,\n      default: false\n    },\n    next: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      icons\n    };\n  },\n  methods: {\n    switchImage(flag) {\n      if (flag === \"prev\") {\n        this.$emit(\"prevFunc\");\n      }\n\n      if (flag === \"next\") {\n        this.$emit(\"nextFunc\");\n      }\n    }\n  },\n  computed: {\n    setStyle() {\n      return function(prev, next) {\n        if (prev && next) {\n          return {\n            display: \"flex\",\n            flex: 2\n          };\n        }\n        if (!prev && !next) {\n          return {\n            display: \"none\"\n          };\n        }\n\n        return {\n          display: \"flex\",\n          flex: 1\n        };\n      };\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.menu_switch_image {\n  display: flex;\n  justify-content: space-around;\n}\n</style>\n"],"sourceRoot":"src/page/menu"}]}